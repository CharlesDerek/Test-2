<% include ../partials/header %>
        <h1 class="h1main">Post Your Property</h1>
    
    <div class="submitform">
            <form action="/listings" method="post">
                <div class="form-group">
                    <label for="name">Name</label>
                    <input class="form-control" type="text" name="name" id="name" placeholder="Yosemite">
                </div>
                <div class="form-group">
                    <label for="image">Image Url</label>
                    <input class="form-control" type="text" name="image" id="image" placeholder="https://source.unsplash.com/TCCCKgdFmBo">
                </div>
                <div class="form-group">
                    <label for="imagetwo">Second Image Url (optional)</label>
                    <input class="form-control" type="text" name="imagetwo" id="imagetwo" placeholder="https://source.unsplash.com/TCCCKgdFmBo">
                </div>
                <div class="form-group">
                    <label for="imagethree">Third Image Url (optional)</label>
                    <input class="form-control" type="text" name="imagethree" id="imagethree" placeholder="https://source.unsplash.com/TCCCKgdFmBo">
                </div>
                <div class="form-group">
                    <label for="imagefour">Fourth Image Url (optional)</label>
                    <input class="form-control" type="text" name="imagefour" id="imagefour" placeholder="https://source.unsplash.com/TCCCKgdFmBo">
                </div>
                <div class="form-group">
                    <label for="imagefive">Fifth Image Url (optional)</label>
                    <input class="form-control" type="text" name="imagefive" id="imagefive" placeholder="https://source.unsplash.com/TCCCKgdFmBo">
                </div>
                <div class="form-group">
                    <label for="cost">Cost</label>
                    <input class="form-control" type="number" name="cost" id="cost" placeholder="9.99" step="0.01" min="0" required>
                </div>
                <div class="form-group">
                    <label for="location">Location</label>
                    <input class="form-control" type="text" name="location" id="location" placeholder="Yosemite National Park, CA">
                </div>
                <div class="form-group">
                    <label for="description">Description</label>
                    <input class="form-control" contenteditable="true" type="text" name="description" id="description" placeholder="Californiaâ€™s Sierra Nevada mountains.">
                </div>
                <div class="form-group">
                    <label for="strictness">Cancellation Strictness Level</label>
                    <select class="form-control" type="text" name="strictness"  id="strictness">
                          <option value="Flexible">Flexible</option>
                          <option value="Moderate">Moderate</option>
                          <option value="Strict (With grace period)">Strict (With grace period)</option>
                          <option value="Very Strict">Very Strict</option>
                    </select>
                </div>
                <div class="form-group">
                    <button class="submitBtn btn btn-lg btn-primary">Submit!</button>
                </div>
                <div>
                <button style="margin-top: 2%;" class="btn btn-xs btn-success" onclick="goBack()">Go Back</button>
            </div>
            </form>
    
    
    
    
    
<script>
function goBack() {
    window.history.back();
}
</script>
<% include ../partials/footer %>
